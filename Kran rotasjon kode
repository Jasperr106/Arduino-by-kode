#include <Servo.h>

Servo myservo; 

int pos = 0;

void setup() {
  myservo.attach(2);
  myservo.write(90);
}

void loop() {
  myservo.write(180);
  delay(5000);  // motoren spinner mot klokka i 5 sekunder
  myservo.write(90);
  delay(1000); // motoren st√•r stille i et sekund
  myservo.write(0);
  delay(5000); // motoren spinner med klokka i 5 sekunder
  myservo.write(90);
  delay(1000);
}
